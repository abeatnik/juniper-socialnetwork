{
    "compilerOptions": {
        "outDir": "./src/",
        "noImplicitAny": true,
        "sourceMap": true,
        "module": "CommonJS",
        "target": "es5",
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "noEmitOnError": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    }
}
